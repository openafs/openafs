        BEGIN
            MENUITEM "&Keine",                      M_DIVIDE_NONE, CHECKED
            MENUITEM "&Vertikal teilen",            M_DIVIDE_H, CHECKED
            MENUITEM "&Horizontal teilen",          M_DIVIDE_V, CHECKED
