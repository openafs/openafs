MENU_MAIN MENU DISCARDABLE 
BEGIN
    POPUP "&Zelle"
    BEGIN
        MENUITEM "Aus&wählen...",               M_CELL_OPEN
        MENUITEM "&Authentifizierung...",       M_CREDENTIALS
        MENUITEM SEPARATOR
        MENUITEM "E&xit",                       M_EXIT
    END
    POPUP "&Anzeige"
    BEGIN
        POPUP "&Server"
        BEGIN
            MENUITEM "&Große Symbole",              M_SVR_VIEW_LARGE
            , CHECKED
            MENUITEM "&Kleine Symbole",             M_SVR_VIEW_SMALL
            , CHECKED
