MENU_MAIN MENU DISCARDABLE
BEGIN
    POPUP "Cell (&C)"
    BEGIN
        MENUITEM "選取(&S)...",                 M_CELL_OPEN
        MENUITEM "鑑別(&A)...",                 M_CREDENTIALS
        MENUITEM SEPARATOR
        MENUITEM "結束(&X)",                    M_EXIT
    END
    POPUP "檢視(&V)"
    BEGIN
        POPUP "伺服器(&S)"
        BEGIN
