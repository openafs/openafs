	$(EXEGUILINK) $(AFSD_SDKLIBS)
        $(_VC_MANIFEST_EMBED_EXE)
	$(EXEPREP)
        $(CODESIGN_USERLAND)
        $(SYMSTORE_IMPORT)

# afsd_service.exe
