ptint.cs.o: ../ptserver/ptint.cs.c
	${AFS_CCRULE} ../ptserver/ptint.cs.c

ptint.xdr.o: ../ptserver/ptint.xdr.c
	${AFS_CCRULE} ../ptserver/ptint.xdr.c

