ListOwned(
  IN afs_int32 id,
  OUT prlist *elist,
  INOUT afs_int32 *lastp
) = 517;

GetCPS2(
  IN afs_int32 id,
  IN afs_int32 host,
  OUT prlist *elist,
  OUT afs_int32 *over
) = 518;

GetHostCPS(
  IN afs_int32 host,
  OUT prlist *elist,
  OUT afs_int32 *over
) = 519;

UpdateEntry(
  IN afs_int32 id,
  IN string name<PR_MAXNAMELEN>,
  IN PrUpdateEntry *uentry
) = 520;

ListEntries(
  IN afs_int32      flags,
  IN afs_int32      startindex,
  OUT prentries *bulkentries,
  OUT afs_int32     *nextstartindex
) = 521;

/* #if defined(SUPERGROUPS) */
ListSuperGroups(
  IN afs_int32 id,
  OUT prlist *elist,
  OUT afs_int32 *over
) = 530;
/* #endif */
