) = 513;

ListElements(
  IN afs_int32 id,
  OUT prlist *elist,
  OUT afs_int32 *over
) = 514;

IsAMemberOf(
  IN afs_int32 uid,
  IN afs_int32 gid,
  OUT afs_int32 *flag
) = 515;

SetFieldsEntry(
  IN afs_int32 id,
  IN afs_int32 mask,
  IN afs_int32 flags,
  IN afs_int32 ngroups,
  IN afs_int32 nusers,
  IN afs_int32 spare1,
  IN afs_int32 spare2
