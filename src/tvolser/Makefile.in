	${TOP_LIBDIR}/libafsauthent.a   \
	${TOP_LIBDIR}/libafsutil.a      \
	${TOP_LIBDIR}/libusd.a          \
	${TOP_LIBDIR}/libafsrpc.a

all: volserver 

all_vos: vos

